<section class="goals float-right col-md-9">
    <div class="my-goals">
        <h3 class="text-white"> Search Results</h3>
        <br>
        <ul>
            {{#each users}}
            <div class = "row">
                <div data-id="{{id}}" class="btn col-xs-10 col-md-10 btn-search-user">
                    <img class="nav-profile-image float-left" src={{image}}>
                    <h3 class="float-left search-result-name">{{firstName}} {{lastName}}</h3>
                </div>

                <div class = "col-xs-2 col-md-1">
                    <button class=" btn add-friend float-right" data-id = "{{id}}">
                    <i class="fa fa-user-plus"></i>
                    </button>
                </div>
            </div>
            {{/each}}
        </ul>
    </div>
</section>
